{
	"name": "minicache",
	"version": "1.1.0",
	"description": "",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"fmt": "biome check --apply .",
		"lint": "biome check .",
		"build": "tsup",
		"test": "vitest",
		"changeset": "changeset",
		"release": "pnpm build && pnpm changeset publish"
	},
	"files": ["dist"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./devalue": {
			"types": "./dist/devalue.d.ts",
			"default": "./dist/devalue.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/minicache.git"
	},
	"keywords": ["cache"],
	"author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ssssota/minicache/issues"
	},
	"homepage": "https://github.com/ssssota/minicache#readme",
	"dependencies": {
		"devalue": "^4.3.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.1",
		"@types/node": "^20.11.19",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3",
		"vite-plugin-doctest": "^1.0.0",
		"vitest": "^2.0.0"
	},
	"packageManager": "pnpm@9.0.6"
}
